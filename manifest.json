{
    "name": "VolfOS",
    "author": "kalle.slotsherre@gmail.com",
    "description": "VolfOS the chrome OS replicator",
    "version": "0.0.1",
    "manifest_version": 3,
    "permissions": ["alwaysOnTopWindows","storage" ,"webview"],
    "app": {
      "background": {
        "scripts": ["background.js"]
      }
      },
      "kiosk_enabled": true,
      
      "icons": { "512": "wolficon.png" },
      "minimum_chrome_version": "28"
  }
  